						android:layout_margin="5dp">
					</TextView>
				</TableRow>


				<TableRow android:id="@+id/tableRow4"
					android:layout_height="wrap_content" android:layout_width="fill_parent"
					android:gravity="center_vertical">
					<TextView android:textAppearance="?android:attr/textAppearanceSmall"
						android:layout_height="wrap_content" android:text="@string/label_sum_start"
						android:id="@+id/textView3" android:layout_width="wrap_content"
						android:layout_margin="5dp">
					</TextView>

					<TextView android:textAppearance="?android:attr/textAppearanceSmall"
						android:layout_height="wrap_content" android:text="0" android:id="@+id/sumstart"
						android:layout_width="wrap_content" android:layout_margin="5dp">
					</TextView>
				</TableRow>



				<TableRow android:id="@+id/tableRow4"
					android:layout_height="wrap_content" android:layout_width="fill_parent"
					android:gravity="center_vertical">
					<TextView android:textAppearance="?android:attr/textAppearanceSmall"
						android:layout_height="wrap_content" android:text="@string/label_sum_end"
						android:id="@+id/textView3" android:layout_width="wrap_content"
						android:layout_margin="5dp">
					</TextView>

					<TextView android:textAppearance="?android:attr/textAppearanceSmall"
						android:layout_height="wrap_content" android:text="0" android:id="@+id/sumend"
						android:layout_width="wrap_content" android:layout_margin="5dp">
					</TextView>
				</TableRow>
			</TableLayout>

